Public Class Form1

    Private Sub Button1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button1.Click

        Dim a As String = ""
        ' 13桁抜き出し用変数
        Dim strmid As String
        ' 13桁をバラバラにして乗算した結果
        Dim result As Long
        ' 最大値
        Dim max As Long = 0

        For i = 1 To a.Length - 12
            strmid = Mid(a, i, i + 12)

            ' 13桁を乗算
            result = Long.Parse(Mid(strmid, 1, 1))
            For j = 2 To 13
                result *= Long.Parse(Mid(strmid, j, 1))
            Next

            ' 過去の結果より大きければ交換
            If max < result Then
                max = result
            End If
        Next

        ' ラベルに最大値を入れる
        Label1.Text = max

    End Sub

End Class
